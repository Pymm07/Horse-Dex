class kingdom:
    def __init__(self):
        self.attributeAnimelia = "Kingdom of horses would be Animelia"
class phylum(kingdom):
    def __init__(self):
        super().__init__()
        self.attributeChordat = "Horse's Phylum would be Chordat and their charaistics would be..."
class clas(phylum):
    def __init__(self):
        super().__init__()
        self.attributeMammalia = "Horse's Class would be Mammalia and their charaistics would be..."
class order(clas):
    def __init__(self):
        super().__init__()
        self.attributePerissodactyla = "Horse's Order would be Perissodactyla and their charaistics would be..."
class family(order):
    def __init__(self):
        super().__init__()
        self.attributeEquidae = "Horse's Family would be Equidae and their charaistics would be..."
class species(family):
    def __init__(self):
        super().__init__()
        self.attributeEquus = "Horse's Species would be Equus ferus caballus and their charaistics would be..."
horse001 = kingdom()
print(horse001.attributeAnimelia)




