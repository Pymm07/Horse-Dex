class kingdom:
    def __init__(self):
        self.attributeAnimelia = "Kingdom of horses would be Animelia"
class phylum(kingdom):
    def __init__(self):
        super().__init__()
        self.attributeChordat = "Horse's Phylum would be Chordat and their charaistics would be..."
class clas(phylum):
    def __init__(self):
        super().__init__()
        self.attributecharaistics = "Horse's Class would be Chordat and their charaistics would be..."
class order(clas):
    def __init__(self):
        super().__init__()
        self.attributecharaistics = "Horse's Order would be Mammalia and their charaistics would be..."
class family(order):
    def __init__(self):
        super().__init__()
        self.attribute = "Horse's Family would be Chordat and their charaistics would be..."
class species(family):
    def __init__(self):
        super().__init__()
        self.attribute = "Horse's Species would be Chordat and their charaistics would be..."
horse001 = kingdom()
print(horse001.attributeHorse)
